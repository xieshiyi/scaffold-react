{
  "name": "scaffold-react",
  "version": "1.0.1",
  "description": "基于react开发的模板",
  "main": "./src/app/app.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open --hot --mode development",
    "build:dev": "rm -rf dist && NODE_ENV=development webpack --progress --config webpack.dev.js --watch --mode development",
    "build:pro": "rm -rf dist && NODE_ENV=production webpack --progress --config webpack.prod.js --watch --mode production",
    "build:test": "rm -rf dist && NODE_ENV=development webpack --config webpack.dev.js --mode development"
  },
  "repository": {
    "type": "git",
    "url": "<your repository url>"
  },
  "author": "<author>",
  "license": "ISC",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-quill": "^1.2.6",
    "react-router-dom": "^4.2.2",
    "uglifyjs-webpack-plugin": "^1.2.5"
  },
  "devDependencies": {
    "antd": "^3.4.3",
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.8",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.0.7",
    "node-sass": "^4.9.0",
    "object-param": "^1.0.2",
    "postcss-loader": "^2.1.4",
    "react-hot-loader": "^3.1.3",
    "react-loadable": "^5.3.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "directories": {
    "doc": "docs"
  },
  "keywords": [
    "react",
    "template",
    "scaffold",
    "前端",
    "模板",
    "antd"
  ]
}
